{
    "$schema": "http://json-schema.org/schema#",
    "type": "array",

    "items": {
        "type": "object",

        "properties": {
            "id":      { "type": "string", "pattern": "^US?ART\\d$" },
            "baud":    { "type": "integer" },
            "alias":   { "type": "string" },
            "comment": { "type": "string" }
        },

        "required": [ "id" ]
    }
}
